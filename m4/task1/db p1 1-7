1-2. Скачав і встановив MySQL на свою віртуальну машину ubuntu
3-4-5.Опис: база данних тюрми  (вибрав так томущо робив схожу в університеті декілька років тому )
	
	DB schema:
	
	use Prison;
	
	create table prisoner ( id_pri int(10), 
	Full_name varchar(30), 
	Age int(10),
	Gender varchar(10),
	Nationality varchar(30), 
	Adress varchar(30), 
	Leisure varchar(10), 
	Termin varchar(30), 
	Amnesty varchar(10), 
	Job int(10), 
	id_camers int(10), 
	id_crimes int(10), 
	id_vis int(10), 
	PRIMARY KEY (id_pri));
	
	insert into prisoner 
	(id_pri ,Full_name,Age,Gender,Nationality ,Adress , Leisure ,Termin , Amnesty ,Job , id_camers , id_crimes , id_vis) 
	values(1, 'Мельник А.О.', 25, 'Чоловік', 'Українець','Ленкавського 6/9', '12 годин', '12 років', 'НІ', 12, 1, 1),
	(2, 'Сиротюк М.М.', 40, 'Чоловік', 'Українець','Шевченка 52', '16 годин', '2 років', 'ТАК', 8, 2, 2),
	(3, 'Шушкин Л.Л.', 32, 'Жінка', 'Українка','Липенка 11', '16 годин', '1 рік', 'НІ', 8, 3, 3),
	(4, 'Стефунька А.А.', 33, 'Чоловік', 'Українець','Липенка 11', '16 годин', '2 рік', 'НІ', 8 , 4, 4),
	(5, 'Кітколюбка М.П.', 58, 'Жінка', 'Українка','Пузика 15', '18 годин', '2 рік', 'НІ', 6 , 5, 5);
	
	create table camers (
	id_cam int(10) AUTO_INCREMENT PRIMARY KEY,
	Type varchar(30),
	Floor int(10),
	Time int(10),
	id_pri int(10),
	id_personal int(10),
	foreign key(id_pri) references prisoner (`id_pri`));

	insert into camers (id_cam, Type, Floor, Time, id_pri, id_personal)
	values(1, 'Однокімнатна', 2, 12.00, 1, 1),
	(2, 'Однокімнатна', 2, 12.00, 2, 2),
	(3, 'Однокімнатна', 2, 12.00, 3, 3),
	(4, 'Однокімнатна', 2, 12.00, 4, 4),
	(5, 'Однокімнатна', 2, 12.00, 5, 5);

	create table personal (
	id_per int(10) AUTO_INCREMENT PRIMARY KEY,
	FLM_per varchar(30),
	Specilality varchar(30),
	Experience int(10),
	id_cam int(10),
	foreign key(id_cam) references camers (id_cam));

	insert into personal (id_per, FLM_per, Specilality, Experience, id_cam)
	values (1, 'Аваков А.А.', 'Охоронець', 15, 1),
	(2, 'Зеленський В.В.', 'Охоронець', 5, 2),
	(3, 'Тимошенко Ю.В.', 'Охоронець', 3, 3),
	(4, 'Медведчук В.Д.', 'Охоронець', 20, 4),
	(5, 'Герасимчук Ю.А.', 'Охоронець', 5, 5);

	create table crimes (
	id_crime int(10) AUTO_INCREMENT PRIMARY KEY,
	Degree varchar(30),
	Article varchar(30),
	Termin int(10),
	id_pri int(10),
	foreign key(id_pri) references prisoner (`id_pri`));



	insert into crimes (id_crime, Degree, Article, Termin, id_pri)
	values (1, 'Особливо тяжкий', 'ст.15 ККУ', 2019, 1),
	(2, 'Легкий', 'ст.28 ККУ', 2019, 2),
	(3, 'Легкий', 'ст.18 ККУ', 2019, 3),
	(4, 'Легкий', 'ст.18 ККУ', 2019, 4),
	(5, 'Легкий', 'ст.28 ККУ', 2019, 5);


	create table visitors (
	id_vis int(10) AUTO_INCREMENT PRIMARY KEY,
	FLM_vis varchar(30),
	Time_vis int(10),
	id_pri int(10),
	foreign key(id_pri) references prisoner (`id_pri`));

	insert into visitors (id_vis, FLM_vis, Time_vis, id_pri)
	values (1, 'Чайпопей А.В.', '10.5.2019', 1),
	(2, 'Пономаренко Р.Р', '28.5.2019', 2),
	(3, 'Мицька А.Л.', '10.6.2019', 3),
	(4, 'Брок К.А.', '20.6.2019', 4),
